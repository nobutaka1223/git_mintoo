<div class="contents row" >
  <% @posts.each do |post| %>
    <div class="content_post" style="background-image": url(<%= post.image %>);">

    </div>
    
    <%= post.title%>
    
    <%= image_tag(post.image) %>
    <%= post.image %>
    
    <%= post.content%>
    
  <% end %>
  
  <%= paginate(@posts) %>
  
  
</div>